{
  "name": "express-app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node --ignore '*.test.ts' --delay 1 src/main.ts",
    "dev2": "nodemon -L -e src src/main.ts",
    "dev1": "nodemon -q -w src src/main.ts",
    "docker:dev": "docker-compose up --build",
    "build": "rimraf dist && swc src -d dist",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "start:dev": "pm2 start ecosystem.config.js",
    "kill": "pm2 kill",
    "test": "jest",
    "monit": "pm2 monit"
  },
  "dependencies": {
    "@types/cli-color": "^2.0.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express-status-monitor": "^1.3.0",
    "cli-color": "^2.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-status-monitor": "^1.3.4",
    "express-validator": "^7.0.1",
    "helmet": "^6.0.1",
    "pm2": "^5.2.2",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.21",
    "@swc/helpers": "^0.4.14",
    "@swc/jest": "^0.2.23",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1"
  }
}
